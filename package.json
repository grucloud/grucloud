{
  "name": "@grucloud/core",
  "version": "0.14.0",
  "description": "Infrastructure as Code",
  "public": true,
  "main": "src/index.js",
  "bin": {
    "gc": "./src/cli/cliEntry.js"
  },
  "scripts": {
    "test:az": "NODE_PATH=src mocha 'src/providers/azure/test/**.test.js'",
    "test:gcp": "NODE_PATH=src mocha 'src/providers/google/test/**.test.js'",
    "test:aws": "NODE_PATH=src mocha 'src/providers/aws/**/**.test.js'",
    "test:scw": "NODE_PATH=src mocha 'src/providers/scaleway/test/**.test.js'",
    "test:mock": "NODE_PATH=src mocha 'src/providers/mock/test/**/*.test.js'",
    "test:mocha": "NODE_PATH=src mocha 'examples/multi/*.test.js' 'src/**/*.test.js' ",
    "test": "NODE_PATH=src nyc mocha 'examples/multi/*.test.js' 'src/**/*.test.js'",
    "test:example": "node test/testExamples.js",
    "start:mock": "nodemon node src/mockServer/MockServerRun.js",
    "cpd": "jscpd -r console --format 'javascript,typescript' ./src",
    "link": "npm link && cd examples && ./link.sh"
  },
  "keywords": [
    "cloud"
  ],
  "author": "Frederic Heem",
  "license": "ISC",
  "devDependencies": {
    "@koa/router": "*",
    "jscpd": "^3.1.0",
    "koa": "^2.11.0",
    "koa-body": "^4.1.1",
    "mocha": "^8.1.0",
    "nodemon": "^2.0.4",
    "nyc": "^15.0.1"
  },
  "dependencies": {
    "aws-sdk": "^2.670.0",
    "axios": "^0.19.2",
    "axios-mock-adapter": "^1.18.1",
    "bluebird": "^3.7.2",
    "chance": "^1.1.6",
    "cli-table3": "^0.6.0",
    "colors": "^1.4.0",
    "commander": "^5.0.0",
    "dotenv": "^8.2.0",
    "duration": "^0.2.2",
    "google-auth-library": "^6.0.0",
    "md5-file": "^5.0.0",
    "node-emoji": "^1.10.0",
    "ping": "^0.2.3",
    "pluralize": "^8.0.0",
    "prompts": "^2.3.2",
    "querystring": "^0.2.0",
    "remedial": "^1.0.8",
    "rubico": "^1.5.8",
    "rxjs": "^6.6.0",
    "shelljs": "^0.8.4",
    "shortid": "^2.2.15",
    "sinon": "^9.0.2",
    "spinnies": "^0.5.1",
    "ssh2": "^0.8.9",
    "url-join": "^4.0.1",
    "uuid": "^7.0.3",
    "winston": "^3.2.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/FredericHeem/grucloud"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "src/cli/cliEntry.js",
      "src/**/*.spec.js",
      "src/**/*test*.js",
      "src/**/test/*.js",
      "src/**/test/config/*.js",
      "src/cli/test/infra/*.js",
      "src/mockServer/MockServerRun.js",
      "src/mockServer/config.js"
    ],
    "extension": [
      ".js"
    ],
    "reporter": [
      "html",
      "text",
      "text-summary"
    ],
    "all": true
  }
}
